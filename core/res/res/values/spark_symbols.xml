<resources>

    <!-- Light / Regular style fonts -->
    <java-symbol type="string" name="config_lightFontFamily" />
    <java-symbol type="string" name="config_regularFontFamily" />

    <!-- Whether to use old mobile type icons as default -->
    <java-symbol type="bool" name="config_useOldMobileIcons" />

    <!-- Whether device has dash charging support -->
    <java-symbol type="bool" name="config_hasDashCharger" />

    <!-- Path to warp charging status file -->
    <java-symbol type="string" name="config_DashChargerStatusPath" />

    <!-- Whether device has warp charging support -->
    <java-symbol type="bool" name="config_hasWarpCharger" />

    <!-- Path to warp charging status file -->
    <java-symbol type="string" name="config_WarpChargerStatusPath" />

    <!-- Whether device has VOOC charging support -->
    <java-symbol type="bool" name="config_hasVoocCharger" />

    <!-- Whether device has turbo power charging support -->
    <java-symbol type="bool" name="config_hasTurboPowerCharger" />

    <!-- Path to fast charging status file to detect whether an oem fast charger is active -->
    <java-symbol type="string" name="config_oemFastChargerStatusPath" />

    <!-- Path to fast charging status file to detect whether an oem fast charger is active -->
    <java-symbol type="string" name="config_oemFastChargerStatusPath2" />

    <!-- Expected value from fast charging status file -->
    <java-symbol type="string" name="config_oemFastChargerStatusValue" />

    <!-- Whether device has physical tri state switch -->
    <java-symbol type="bool" name="config_hasAlertSlider" />

    <!-- The location of the devices physical tri state switch -->
    <java-symbol type="integer" name="config_alertSliderLocation" />

    <!-- Whether key handler sends intent when changing slider position -->
    <java-symbol type="string" name="config_alertSliderIntent" />

    <!-- Pocket Lock -->
    <java-symbol type="layout" name="pocket_lock_view" />

    <!-- Pocket bridge -->
    <java-symbol type="string" name="config_pocketBridgeSysfsInpocket" />
    <java-symbol type="bool" name="config_pocketModeSupported" />
    <java-symbol type="bool" name="config_pocketUseLightSensor" />

    <!-- Pocket Service -->
    <java-symbol type="string" name="config_pocketJudgeVendorSensorName" />
    <java-symbol type="string" name="config_pocketJudgeVendorProximitySensorName" />

    <!-- Whether the device supports Smart Pixels -->
    <java-symbol type="bool" name="config_supportSmartPixels" />

    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Enable face auth only when swiping security view -->
    <java-symbol type="bool" name="config_faceAuthOnlyOnSecurityView" />

    <!-- Sensor block -->
    <java-symbol type="array" name="config_blockPackagesSensorDrain" />

    <!-- Parallel space badge -->
    <java-symbol type="string" name="parallel_space_badge_label" />
    <java-symbol type="drawable" name="ic_parallel_badge" />
    <java-symbol type="drawable" name="ic_parallel_icon_badge" />

    <!-- Parallel space badge colors -->
    <java-symbol type="color" name="parallel_badge_1" />
    <java-symbol type="color" name="parallel_badge_2" />
    <java-symbol type="color" name="parallel_badge_3" />
    <java-symbol type="color" name="parallel_badge_4" />
    <java-symbol type="color" name="parallel_badge_5" />
    <java-symbol type="color" name="parallel_badge_6" />
    <java-symbol type="color" name="parallel_badge_7" />
    <java-symbol type="color" name="parallel_badge_8" />
    <java-symbol type="color" name="parallel_badge_9" />

    <!-- Parallel Space -->
    <java-symbol type="integer" name="config_parallelSpaceMaxCount" />
    <java-symbol type="array" name="config_parallelSpaceWhitelist" />
    <java-symbol type="array" name="config_parallelSpaceBlocklist" />

    <!-- App version -->
    <java-symbol type="string" name="old_version_number" />
    <java-symbol type="string" name="new_version_number" />

    <!-- App lock -->
    <java-symbol type="string" name="unlock_application" />
    <java-symbol type="array" name="config_appLockAllowedSystemApps" />

    <!-- Settings Restart -->
    <java-symbol type="string" name="settings_restart_title" />
    <java-symbol type="string" name="settings_restart_message" />

    <!-- Soft restart dialog -->
    <java-symbol type="string" name="system_restart_title" />
    <java-symbol type="string" name="system_restart_message" />

  <!-- Default UDFPS pressed icon -->
    <java-symbol type="integer" name="config_udfps_pressed_color" />
</resources>
